{
    "manifest_version":2,
    "version":"1.0",
    "name":"Mem-xtension",
    "content_scripts":[
     {
      "matches":["<all_urls>"],
      "js":["main.js"]
     }
    ],
    "web_accessible_resources": [
          "pp.jpg"
    ],
    "icons": {
        "48": "productivity.png"
    },
    "browser_action":{
        "default_icon":{
          "19":"productivity.png"
        }
    },
    "background":{
        "scripts":["background.js"]
    },
    "permissions":[
        "tabs"
    ]
  }